{
  "id": 105,
  "name": "الفيل",
  "transliteration": "Al-Fil",
  "translation": "The Elephant",
  "verses": [
    "ʾa-lam tara kayfa faʿala rabbuka bi-ʾaṣḥābi l-fīli",
    "ʾa-lam yajʿal kaydahum fī taḍlīlin",
    "wa-ʾarsala ʿalayhim ṭayran ʾabābīla",
    "tarmīhim bi-ḥijāratin min sijjīlin",
    "fa-jaʿalahum ka-ʿaṣfin maʾkūlin"
  ]
}