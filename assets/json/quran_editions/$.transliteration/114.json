{
  "id": 114,
  "name": "الناس",
  "transliteration": "An-Nas",
  "translation": "Mankind",
  "verses": [
    "qul ʾaʿūdhu bi-rabbi n-nāsi",
    "maliki n-nāsi",
    "ʾilāhi n-nāsi",
    "min sharri l-waswāsi l-khannāsi",
    "alladhī yuwaswisu fī ṣudūri n-nāsi",
    "mina l-jinnati wa-n-nāsi"
  ]
}