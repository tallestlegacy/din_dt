{
  "id": 102,
  "name": "التكاثر",
  "transliteration": "At-Takathur",
  "translation": "The Rivalry in world increase",
  "verses": [
    "ʾalhākumu t-takāthuru",
    "ḥattā zurtumu l-maqābira",
    "kallā sawfa taʿlamūna",
    "thumma kallā sawfa taʿlamūna",
    "kallā law taʿlamūna ʿilma l-yaqīni",
    "la-tarawunna l-jaḥīma",
    "thumma la-tarawunnahā ʿayna l-yaqīni",
    "thumma la-tusʾalunna yawmaʾidhin ʿani n-naʿīmi"
  ]
}