{
  "id": 107,
  "name": "الماعون",
  "transliteration": "Al-Ma'un",
  "translation": "The Small kindnesses",
  "verses": [
    "ʾa-raʾayta lladhī yukadhdhibu bi-d-dīni",
    "fa-dhālika lladhī yaduʿʿu l-yatīma",
    "wa-lā yaḥuḍḍu ʿalā ṭaʿāmi l-miskīni",
    "fa-waylun li-l-muṣallīna",
    "alladhīna hum ʿan ṣalātihim sāhūna",
    "alladhīna hum yurāʾūna",
    "wa-yamnaʿūna l-māʿūna"
  ]
}