{
  "id": 109,
  "name": "الكافرون",
  "transliteration": "Al-Kafirun",
  "translation": "The Disbelievers",
  "verses": [
    "qul yā-ʾayyuhā l-kāfirūna",
    "lā ʾaʿbudu mā taʿbudūna",
    "wa-lā ʾantum ʿābidūna mā ʾaʿbudu",
    "wa-lā ʾana ʿābidun mā ʿabadtum",
    "wa-lā ʾantum ʿābidūna mā ʾaʿbudu",
    "lakum dīnukum wa-liya dīni"
  ]
}