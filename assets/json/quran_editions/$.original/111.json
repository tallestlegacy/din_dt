{
  "id": 111,
  "name": "المسد",
  "transliteration": "Al-Masad",
  "translation": "The Palm Fiber",
  "verses": [
    "تَبَّت يَدا أَبي لَهَبٍ وَتَبَّ",
    "ما أَغنىٰ عَنهُ مالُهُ وَما كَسَبَ",
    "سَيَصلىٰ نارًا ذاتَ لَهَبٍ",
    "وَامرَأَتُهُ حَمّالَةَ الحَطَبِ",
    "في جيدِها حَبلٌ مِن مَسَدٍ"
  ]
}