{
  "id": 109,
  "name": "الكافرون",
  "transliteration": "Al-Kafirun",
  "translation": "The Disbelievers",
  "verses": [
    "قُل يا أَيُّهَا الكافِرونَ",
    "لا أَعبُدُ ما تَعبُدونَ",
    "وَلا أَنتُم عابِدونَ ما أَعبُدُ",
    "وَلا أَنا عابِدٌ ما عَبَدتُم",
    "وَلا أَنتُم عابِدونَ ما أَعبُدُ",
    "لَكُم دينُكُم وَلِيَ دينِ"
  ]
}